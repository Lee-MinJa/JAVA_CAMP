<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.mybatis.mapper.TagMapper"> 
 <select id="TagList" parameterType="map" resultType="map">
SELECT t.TAG_NAME FROM tag t , tagging g
WHERE t.tag_code=g.tag_code
AND g.PROMO_NUM =313
<if test="MEM_NUM !=null">
WHERE MEM_NUM = #{MEM_NUM}
</if>

</select>





</mapper>
